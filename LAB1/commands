#run the input generator in Python_scripts folder
python3 input_gen.py

#to run the simulation
cd simulation/
source /software/scripts/init msim6.2g.
vlib work
#compile vhdl files
vcom -93 -work ./work #path of the file
#compile verilog file
vlog -work ./work #path of the file
#run the simulation e.g. tb_iir (name of the top module)
vsim work.tb_iir
#other commands from graphical interface


#run the C model in Programma_C folder

#run error checker in Python_scripts folder
python3 error_check.py
